-- query to show all course details
SELECT 
    c.course_id,
    c.course_name,
    c.course_board,
    c.course_level,
    u.unit_id,
    u.unit_number,
    u.unit_name,
    cr.criteria_id,
    cr.criteria_name,
    sc.subcriteria_id,
    sc.subcriteria_name
FROM 
    course c
    INNER JOIN unit u ON c.course_id = u.course_id
    INNER JOIN criteria cr ON u.unit_id = cr.unit_id
    INNER JOIN subcriteria sc ON cr.criteria_id = sc.criteria_id;